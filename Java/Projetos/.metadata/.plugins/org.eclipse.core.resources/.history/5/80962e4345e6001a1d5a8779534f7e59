
public class While {

	public static void main(String[] args) {
		int x = 0;
		int y = 0;
		
		while(x<10) {
			System.out.println(x+ " ");
			x++;
		}
		
		//Usando break para sair no 5
		System.out.println(" ");
		while(y<10) {
			System.out.println(x+" ");
			if (y==5) {
				break;
			}
			y++;
		}

	}

}
